{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@/styled-system/*": ["styled-system/*"]
		}
	},
	"include": [
		"src",
		"styled-system",
		"src/**/*.d.ts",
		"src/**/*.ts",
		"src/**/*.astro"
	]
}