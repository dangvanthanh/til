{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@/styled-system/*": ["./styled-system/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"allowSyntheticDefaultImports": true,
    "lib": ["WebWorker"]
	},
	"include": ["src", "styled-system", "algolia/algolia.ts"]
}
